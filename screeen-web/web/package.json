	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^1.1.5",
		"@vue/compiler-sfc": "^3.0.5",
		"prettier": "^2.2.1",
		"typescript": "^4.1.3",
		"vite": "^2.0.5",
		"vite-plugin-windicss": "^0.6.4"
	}
